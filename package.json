{
  "name": "quiz-analysis-api",
  "version": "1.0.1",
  "description": "",
  "main": "dist/app/analysis-api/src/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build && tscpaths -p tsconfig.json -s ./src -o ./dist && ts-project-bundle --root=../ --project=./ --out=./bundle",
    "build:es": "esbuild src/index.ts --outfile=dist/index.js --bundle --loader:.ts=ts --platform=node --external:saslprep --external:swagger-ui-dist",
    "start": "nodemon ./dist/index.js",
    "dev": "nodemon --exec ts-node -- -r tsconfig-paths/register ./src/index.ts --dir .",
    "prod": "npm run build && npm run start",
    "clean": "rm -rf node_modules coverage dist .nyc_output",
    "lint": "eslint 'src/**/*.ts' --fix",
    "ingestData": "ts-node -r tsconfig-paths/register src/scripts/ingestData.ts",
    "createTagInfo": "ts-node -r tsconfig-paths/register src/scripts/createTagInfo.ts",
    "test": "env TS_NODE_PROJECT='./tsconfig.base.json' mocha -r ts-node/register -r tsconfig-paths/register 'src/**/*.spec.ts'",
    "docker:up": "TAG=latest docker compose --env-file=./.env -f ./docker/docker-compose.yml up -d",
    "docker:down": "TAG=latest docker compose --env-file=./.env -f ./docker/docker-compose.yml down",
    "docker:build": "TAG=latest docker compose --env-file=./.env -f ./docker/docker-compose.yml build",
    "docker:push": "TAG=latest docker compose --env-file=./.env -f ./docker/docker-compose.yml push"
  },
  "keywords": [],
  "author": "Landon Bland",
  "license": "MIT",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.190",
    "@types/mongoose": "^5.10.1",
    "@types/morgan": "^1.9.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-dist": "^3.30.1",
    "@types/swagger-ui-express": "^4.1.3",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.15",
    "eslint": "^8.28.0",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "compromise": "^14.8.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.0",
    "http-status": "^1.5.3",
    "joi": "^17.7.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "saslprep": "^1.0.3",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0"
  }
}
